# API Configuration
REACT_APP_API_URL=http://localhost:3001/api

# Environment
REACT_APP_ENV=development

# App Configuration
REACT_APP_NAME=Lab Management System
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_DARK_MODE=true

# Authentication
REACT_APP_AUTH_TOKEN_KEY=lab_management_token

# External Services
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id

# Autenticación
REACT_APP_REFRESH_TOKEN_KEY=securelearn_refresh_token

# Configuración
REACT_APP_DESCRIPTION=Plataforma educativa de ciberseguridad

# Analytics
REACT_APP_GA_TRACKING_ID=your-ga-tracking-id

# Recaptcha
REACT_APP_RECAPTCHA_SITE_KEY=your-recaptcha-site-key

# Socket.IO
REACT_APP_SOCKET_URL=ws://localhost:4000

# Feature Flags
REACT_APP_ENABLE_RECAPTCHA=false 